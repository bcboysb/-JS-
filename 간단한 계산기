const calculator = {
  add: function (a, b){
    return a+b;
  },
  minus: function (a, b){
    return a-b;
  },
  divide: function (a, b){
    return a/b;
  },
  multiply: function (a, b){
    return a*b;
  },
  powerOf: function (a, b){
    return a**b;
  }
}
calculator.add(2,10);
calculator.minus(2,10);
calculator.divide(2,10);
calculator.multiply(2,10);
calculator.powerOf(2,10); 
